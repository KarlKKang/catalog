{
  "name": "catalog",
  "version": "1.0.0",
  "description": "Front-end of featherine.com",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "type-check": "tsc --noEmit",
    "build:type": "tsc --emitDeclarationOnly --outDir dist/script",
    "build-dev:type": "tsc --emitDeclarationOnly --outDir dev/script",
    "build:font": "python build_font.py && node minify-font-css.js",
    "build:js": "webpack --progress --config webpack.prod.cjs",
    "build-dev:js": "webpack --progress --config webpack.dev.cjs",
    "build:css": "node minify-css.js",
    "build-dev:css": "npm run build:css -- dev",
    "build:legacy": "node build_legacy.js",
    "build-dev:legacy": "npm run build:legacy -- dev",
    "build-dev": "npm run build-dev:css && npm run lint && npm run type-check && npm run build-dev:js",
    "build": "npm run build:css && npm run lint && npm run type-check && npm run build:js",
    "lint": "npx eslint src/script/ --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "publish": "sh publish.sh featherine-website ./dist/",
    "publish-dev": "sh publish.sh featherine-website-alpha ./dev/"
  },
  "author": "Kai Kang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KarlKKang/catalog.git"
  },
  "bugs": {
    "url": "https://github.com/KarlKKang/catalog/issues"
  },
  "homepage": "https://github.com/KarlKKang/catalog",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/runtime": "^7.21.5",
    "@types/node-forge": "^1.3.2",
    "@types/ua-parser-js": "^0.7.36",
    "@types/video.js": "^7.3.51",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "babel-loader": "^9.1.2",
    "babel-plugin-const-enum": "^1.2.0",
    "core-js": "^3.30.2",
    "cssnano": "^6.0.1",
    "cssnano-preset-advanced": "^6.0.1",
    "ejs-loader": "^0.5.0",
    "eslint": "^8.40.0",
    "hls.js": "^1.4.2",
    "html-loader": "^4.2.0",
    "html-minifier-terser": "^7.2.0",
    "html-webpack-plugin": "^5.5.1",
    "intersection-observer": "^0.12.2",
    "isbot": "^3.6.10",
    "node-forge": "^1.3.1",
    "postcss": "^8.4.23",
    "screenfull": "^6.0.2",
    "terser-webpack-plugin": "^5.3.8",
    "typescript": "^5.0.4",
    "ua-parser-js": "^1.0.35",
    "video.js": "^7.21.4",
    "webp-hero": "^0.0.2",
    "webpack": "^5.82.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.1.1",
    "webpack-merge": "^5.8.0"
  }
}