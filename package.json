{
  "name": "catalog",
  "version": "1.0.0",
  "description": "Front-end of featherine.com",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build:font": "python3 build_font.py",
    "build": "npm run lint && tsc --noEmit && node build_legacy.js && webpack --progress --config webpack.prod.cjs && node sw_transpile.js",
    "build-dev": "npm run lint && tsc --noEmit && node build_legacy.js dev && webpack --progress --config webpack.dev.cjs && node sw_transpile.js dev",
    "lint": "npx eslint src/script/ --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "publish": "sh publish.sh featherine-website ./dist/",
    "publish-dev": "sh publish.sh featherine-website-alpha ./dev/",
    "init-sync": "sh init_sync.sh featherine-website ./dist/",
    "init-sync-dev": "sh init_sync.sh featherine-website-alpha ./dev/"
  },
  "author": "Kai Kang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KarlKKang/catalog.git"
  },
  "bugs": {
    "url": "https://github.com/KarlKKang/catalog/issues"
  },
  "homepage": "https://github.com/KarlKKang/catalog",
  "devDependencies": {
    "@babel/core": "7.22.9",
    "@babel/plugin-transform-runtime": "7.22.9",
    "@babel/preset-env": "7.22.9",
    "@babel/preset-typescript": "7.22.5",
    "@babel/runtime": "7.22.6",
    "@types/qrcode": "1.5.2",
    "@types/ua-parser-js": "0.7.36",
    "@types/video.js": "7.3.52",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "6.1.0",
    "babel-loader": "9.1.3",
    "circular-dependency-plugin": "5.2.2",
    "core-js": "3.31.1",
    "css-loader": "6.8.1",
    "css-minimizer-webpack-plugin": "5.0.1",
    "cssnano": "6.0.1",
    "cssnano-preset-advanced": "6.0.1",
    "ejs-loader": "0.5.0",
    "eslint": "8.45.0",
    "favicons-webpack-plugin": "6.0.1",
    "hls.js": "1.4.9",
    "html-loader": "4.2.0",
    "html-minifier-terser": "7.2.0",
    "html-webpack-plugin": "5.5.3",
    "intersection-observer": "0.12.2",
    "isbot": "3.6.12",
    "mini-css-extract-plugin": "2.7.6",
    "postcss": "8.4.26",
    "qrcode": "1.5.3",
    "sass": "1.67.0",
    "sass-loader": "13.3.2",
    "screenfull": "6.0.2",
    "terser-webpack-plugin": "5.3.9",
    "typescript": "5.1.6",
    "ua-parser-js": "1.0.35",
    "video.js": "7.21.4",
    "webp-hero": "0.0.2",
    "webpack": "5.88.2",
    "webpack-bundle-analyzer": "4.9.0",
    "webpack-cli": "5.1.4",
    "workbox-webpack-plugin": "7.0.0",
    "workbox-window": "7.0.0"
  }
}