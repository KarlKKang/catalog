    "@babel/runtime": "^7.19.0",
    "@types/node-forge": "^1.0.5",
    "@types/video.js": "^7.3.47",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-loader": "^8.2.5",
