    "eslint": "^8.23.1",
    "hls.js": "^1.2.3",
    "html-minifier-terser": "^7.0.0",
    "html-webpack-plugin": "^5.5.0",
    "intersection-observer": "^0.12.2",
    "node-forge": "^1.3.1",
    "postcss": "^8.4.16",
