{
  "name": "catalog",
  "version": "1.0.0",
  "description": "Front-end of featherine.com",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build:font": "python3 build_font.py",
    "build": "npm run lint && npm run check-circular && node build_legacy.js && webpack --progress --config webpack.prod.js --config-name main && webpack --progress --config webpack.prod.js --config-name sw && sh check_syntax.sh dist/**/*.js",
    "build-dev": "npm run lint && npm run check-circular && node build_legacy.js dev && webpack --progress --config webpack.dev.js --config-name main && webpack --progress --config webpack.dev.js --config-name sw && sh check_syntax.sh dev/**/*.js",
    "build:hls.js": "cd hls.js && ALT_AUDIO=1 npm run build",
    "lint": "npx eslint src/script/",
    "lint:fix": "npm run lint -- --fix",
    "publish": "sh publish.sh featherine-website ./dist/",
    "publish-dev": "sh publish.sh featherine-website-alpha ./dev/",
    "init-sync": "sh init_sync.sh featherine-website ./dist/",
    "init-sync-dev": "sh init_sync.sh featherine-website-alpha ./dev/",
    "check-circular": "madge --circular --warning src/script/entry/index.ts"
  },
  "author": "Kai Kang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KarlKKang/catalog.git"
  },
  "bugs": {
    "url": "https://github.com/KarlKKang/catalog/issues"
  },
  "homepage": "https://github.com/KarlKKang/catalog",
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/plugin-transform-runtime": "7.25.9",
    "@babel/preset-env": "7.26.0",
    "@babel/runtime": "7.26.0",
    "@eslint/js": "9.17.0",
    "@stylistic/eslint-plugin": "2.12.1",
    "@types/eslint__js": "8.42.3",
    "@types/qrcode": "1.5.5",
    "@types/ua-parser-js": "0.7.39",
    "babel-loader": "9.2.1",
    "core-js": "3.39.0",
    "css-loader": "7.1.2",
    "css-minimizer-webpack-plugin": "7.0.0",
    "cssnano": "7.0.6",
    "cssnano-preset-advanced": "7.0.6",
    "ejs-loader": "0.5.0",
    "eslint": "9.17.0",
    "favicons-webpack-plugin": "6.0.1",
    "globals": "15.14.0",
    "html-loader": "5.1.0",
    "html-minifier-terser": "7.2.0",
    "html-webpack-plugin": "5.6.3",
    "isbot": "5.1.18",
    "madge": "8.0.0",
    "mini-css-extract-plugin": "2.9.2",
    "postcss": "8.4.49",
    "qrcode": "1.5.4",
    "sass": "1.83.0",
    "sass-loader": "16.0.4",
    "screenfull": "6.0.2",
    "source-map-loader": "5.0.0",
    "terser-webpack-plugin": "5.3.11",
    "ts-loader": "9.5.1",
    "typescript": "5.5.4",
    "typescript-eslint": "8.18.2",
    "typescript-plugin-css-modules": "5.1.0",
    "ua-parser-js": "1.0.40",
    "webp-hero": "0.0.2",
    "webpack": "5.97.1",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "6.0.1",
    "workbox-webpack-plugin": "7.3.0",
    "workbox-window": "7.3.0"
  }
}