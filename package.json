{
  "name": "catalog",
  "version": "1.0.0",
  "description": "Front-end of featherine.com",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "type-check": "tsc --noEmit",
    "build:type": "tsc --emitDeclarationOnly --outDir dist/script",
    "build-dev:type": "tsc --emitDeclarationOnly --outDir dev/script",
    "build:font": "python build_font.py && node minify-font-css.js",
    "build:js": "webpack --progress --config webpack.prod.cjs",
    "build-dev:js": "webpack --progress --config webpack.dev.cjs",
    "build:css": "node minify-css.js",
    "build-dev:css": "npm run build:css -- dev",
    "build:legacy": "node build_legacy.js",
    "build-dev:legacy": "npm run build:legacy -- dev",
    "build-dev": "npm run build-dev:css && npm run lint && npm run type-check && npm run build-dev:js",
    "build": "npm run build:css && npm run lint && npm run type-check && npm run build:js",
    "lint": "npx eslint src/script/ --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "publish": "sh publish.sh featherine-website ./dist/",
    "publish-dev": "sh publish.sh featherine-website-alpha ./dev/"
  },
  "keywords": [],
  "author": "Kai Kang",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.20.13",
    "@types/node-forge": "^1.3.1",
    "@types/ua-parser-js": "^0.7.36",
    "@types/video.js": "^7.3.51",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "babel-loader": "^9.1.2",
    "core-js": "^3.27.2",
    "cssnano": "^5.1.14",
    "cssnano-preset-advanced": "^5.3.9",
    "dashjs": "^4.5.2",
    "ejs-loader": "^0.5.0",
    "eslint": "^8.34.0",
    "hls.js": "^1.3.3",
    "html-loader": "^4.2.0",
    "html-minifier-terser": "^7.1.0",
    "html-webpack-plugin": "^5.5.0",
    "intersection-observer": "^0.12.2",
    "isbot": "^3.6.5",
    "node-forge": "^1.3.1",
    "postcss": "^8.4.21",
    "screenfull": "^6.0.2",
    "terser-webpack-plugin": "^5.3.6",
    "typescript": "^4.9.5",
    "ua-parser-js": "^1.0.33",
    "video.js": "^7.21.2",
    "webp-hero": "^0.0.2",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.8.0"
  }
}