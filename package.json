{
  "name": "catalog",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "type-check": "tsc --noEmit",
    "build:type": "tsc --emitDeclarationOnly --outDir dist/script",
    "build-dev:type": "tsc --emitDeclarationOnly --outDir dev/script",
    "build:font": "python build_font.py && node minify-font-css.js",
    "build:js": "webpack --config webpack.prod.cjs",
    "build-dev:js": "webpack --config webpack.dev.cjs",
    "build:css": "node minify-css.js",
    "build-dev:css": "npm run build:css -- dev",
    "build:legacy": "node build_legacy.js",
    "build-dev:legacy": "npm run build:legacy -- dev",
    "build-dev": "npm run build-dev:css && npm run lint && npm run type-check && npm run build-dev:js",
    "build": "npm run build:css && npm run lint && npm run type-check && npm run build:js",
    "start-server": "npx http-server dev -g -b -c-1 -S -C cert.pem",
    "lint": "./node_modules/.bin/eslint src/script/ --ext .ts",
    "lint:fix": "npm run lint -- --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
