{
  "name": "catalog",
  "version": "1.0.0",
  "description": "Front-end of featherine.com",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build:font": "python3 build_font.py",
    "build": "npm run lint && node build_legacy.js && webpack --progress --config webpack.prod.js --config-name main && webpack --progress --config webpack.prod.js --config-name sw && node -c dist/**/*.js",
    "build-dev": "npm run lint && node build_legacy.js dev && webpack --progress --config webpack.dev.js --config-name main && webpack --progress --config webpack.dev.js --config-name sw && node -c dev/**/*.js",
    "build:hls.js": "cd hls.js && ALT_AUDIO=1 npm run build",
    "lint": "npx eslint src/script/ --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "publish": "sh publish.sh featherine-website ./dist/",
    "publish-dev": "sh publish.sh featherine-website-alpha ./dev/",
    "init-sync": "sh init_sync.sh featherine-website ./dist/",
    "init-sync-dev": "sh init_sync.sh featherine-website-alpha ./dev/"
  },
  "author": "Kai Kang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KarlKKang/catalog.git"
  },
  "bugs": {
    "url": "https://github.com/KarlKKang/catalog/issues"
  },
  "homepage": "https://github.com/KarlKKang/catalog",
  "devDependencies": {
    "@babel/core": "7.24.7",
    "@babel/plugin-transform-runtime": "7.24.7",
    "@babel/preset-env": "7.24.7",
    "@babel/runtime": "7.24.7",
    "@types/qrcode": "1.5.5",
    "@types/ua-parser-js": "0.7.39",
    "@typescript-eslint/eslint-plugin": "7.13.0",
    "@typescript-eslint/parser": "7.13.0",
    "babel-loader": "9.1.3",
    "circular-dependency-plugin": "5.2.2",
    "core-js": "3.37.1",
    "css-loader": "7.1.2",
    "css-minimizer-webpack-plugin": "7.0.0",
    "cssnano": "7.0.2",
    "cssnano-preset-advanced": "7.0.2",
    "ejs-loader": "0.5.0",
    "eslint": "8.57.0",
    "favicons-webpack-plugin": "6.0.1",
    "html-loader": "5.0.0",
    "html-minifier-terser": "7.2.0",
    "html-webpack-plugin": "5.6.0",
    "isbot": "5.1.9",
    "mini-css-extract-plugin": "2.9.0",
    "postcss": "8.4.38",
    "qrcode": "1.5.3",
    "sass": "1.77.5",
    "sass-loader": "14.2.1",
    "screenfull": "6.0.2",
    "source-map-loader": "5.0.0",
    "terser-webpack-plugin": "5.3.10",
    "ts-loader": "9.5.1",
    "typescript": "5.4.5",
    "typescript-plugin-css-modules": "5.1.0",
    "ua-parser-js": "1.0.38",
    "webp-hero": "0.0.2",
    "webpack": "5.92.0",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "5.1.4",
    "workbox-webpack-plugin": "7.1.0",
    "workbox-window": "7.1.0"
  }
}